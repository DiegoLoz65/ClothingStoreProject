@model IEnumerable<producto>
@{
    ViewData["Title"] = "BubbleSort";
}
<h2 class="font-weight-bold display-4">BubbleSort</h2>


<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Nombre</th>
      <th scope="col">Precio $</th>
      <th scope="col">Color</th>
      <th scope="col">Talla</th>
      <th scope="col">Unidades</th>
    </tr>
  </thead>
  <tbody>

@foreach (producto item in Model)
    {   
    <tr>
      <td>@item.tmpnombre</td>
      <td>@item.tmpprecio</td>
      <td>@item.tmpcolor</td>
      <td>@item.tmptalla</td>
      <td>@item.tmpunidades</td>
    </tr>
    }

  </tbody>
</table>